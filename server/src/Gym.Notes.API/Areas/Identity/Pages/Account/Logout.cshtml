@page
@model LogoutModel
@{
    ViewData["Title"] = "Log out";
}

<div class="limiter">
    <div class="login-container">
        <span class="login-form-title p-b-53 center">
            You have successfully logged out of the application
        </span>

        <div class="login-container-form-button m-t-17 center">
            <div class="login-container">
                <a asp-page="./Login" class="txt3 small-border">
                    Back to login page
                </a>
            </div>
        </div>
    </div>
</div>
